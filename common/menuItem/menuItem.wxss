.menuNavList {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.menuItem {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.menuItem_img {
  width: 90rpx;
  height: 90rpx;
  flex-shrink: 0;
}

.menuItem_text {
  font-size: 22rpx;
}

.munetitle {
  width: 100%;
  text-align: left;
  padding: 20rpx;
  display: flex;
  align-items: center;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #333;
}

.fenleiTitle {
  font-size: 36rpx;
  color: #ffa2a7;
  margin-right: 10rpx;
}
